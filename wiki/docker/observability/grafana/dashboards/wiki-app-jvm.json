{
  "id": null,
  "uid": "wiki-app-jvm",
  "title": "Wiki API - Application & JVM",
  "tags": ["wiki", "app", "jvm"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-30m", "to": "now" },
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP RPS",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "refId": "A", "expr": "sum(rate(http_server_requests_seconds_count[1m]))" }]
    },
    {
      "type": "timeseries",
      "title": "HTTP P95 Latency (s)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "refId": "A", "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket[5m])))" }]
    },
    {
      "type": "timeseries",
      "title": "JVM Heap Used",
      "gridPos": { "x": 0, "y": 8, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "refId": "A", "expr": "sum(jvm_memory_used_bytes{area=\"heap\"})" }]
    },
    {
      "type": "timeseries",
      "title": "JVM Threads Live",
      "gridPos": { "x": 8, "y": 8, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "refId": "A", "expr": "jvm_threads_live_threads" }]
    },
    {
      "type": "timeseries",
      "title": "Process CPU Usage",
      "gridPos": { "x": 16, "y": 8, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [{ "refId": "A", "expr": "process_cpu_usage" }]
    }
  ]
}
